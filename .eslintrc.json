{
	"root": true,
	"extends": "./node_modules/eslint-config-mdcs/index.js",
  "overrides": [
		{
			"files": ["*.ts"],
			"parser": "@typescript-eslint/parser",
			"plugins": [
				"@typescript-eslint"
			],
			"rules": {
				"no-unused-vars": ["error", { "args": "none" }],
				"indent": [2, 2]
			}
		}
	]
}